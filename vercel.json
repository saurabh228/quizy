{
    "builds": [
        {
            "src": "quizy/wsgi.py",
            "use": "@vercel/python",
            "config": { "maxLambdaSize": "15mb" , "runtime": "python3.12.7" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/quizy/wsgi.py"
        }
    ]
}